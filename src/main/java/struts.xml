<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
		
	<package name="default" extends="json-default" namespace="/">
		<action name="login" class="com.action.logging2">
			<result name="failure" type="json">/index.jsp</result>
			<result name="input" type="json">/index.jsp</result>
			<result name="success" type="json">	</result>
 		</action>
 		
 		<action name="signup" class="com.action.signing" >
			<result name="failure" type="json">/signup.jsp</result>
			<result name="input" type="json">/index.jsp</result>
			<result name="success" type="redirect"></result>
 		</action>
	</package> 
		
	
</struts>